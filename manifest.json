{
    "name": "Arabic meaning pop-up",
    "version": "1.0",
    "description": "Meaning Pop-up",
    "permissions": [
        "contextMenus",
        "activeTab",
        "declarativeContent", 
        "storage",
        "notifications"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "manifest_version": 2
  }